<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>Client</servlet-name>
        <servlet-class>com.cal.Client</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientOne</servlet-name>
        <servlet-class>com.cal.ClientOne</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientSide</servlet-name>
        <servlet-class>com.client.ClientSide</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ClientServlet</servlet-name>
        <servlet-class>com.cal.ClientServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Serv</servlet-name>
        <servlet-class>client.Serv</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Client</servlet-name>
        <url-pattern>/Client</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientOne</servlet-name>
        <url-pattern>/ClientOne</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientSide</servlet-name>
        <url-pattern>/ClientSide</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ClientServlet</servlet-name>
        <url-pattern>/ClientServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Serv</servlet-name>
        <url-pattern>/Serv</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
